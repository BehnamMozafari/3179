{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 600,
    "height": 450,
    "title": "Covid Cases against Deaths per Country (March, 2020)",
    "data": {
        "url": "./Covid-Live-Long.csv"
    },
    "mark": {
        "type": "boxplot"
    },
    "encoding": {
        "x": {
            "field": "name",
            "type": "nominal"
        },
        "color": {
            "field": "name",
            "type": "nominal",
            "legend": null
        },
        "y": {
            "field": "value",
            "type": "quantitative",
            "axis": {
                "tickCount": 4
            }
        }
    }
}